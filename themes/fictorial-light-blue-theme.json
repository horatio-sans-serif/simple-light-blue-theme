{
  "name": "Fictorial Light (Blue)",
  "colors": {
    "editor.background": "#f4fbff",
    "editor.foreground": "#7f95a1",
    "statusBar.background": "#e6f2f9",
    "statusBar.foreground": "#7f95a1",
    "statusBar.debuggingBackground": "#7f95a1",
    "statusBar.debuggingForeground": "#e6f2f9",
    "activityBar.background": "#e6f2f9",
    "activityBar.foreground": "#7f95a1",
    "tab.inactiveBackground": "#f4fbff",
    "tab.inactiveForeground": "#798c96",
    "tab.activeBackground": "#e6f2f9",
    "tab.activeForeground": "#7f95a1",
    "editorLineNumber.foreground": "#7f95a1",
    "editor.lineHighlightBackground": "#90c2dc33",
    "sideBar.background": "#e6f2f9",
    "sideBar.foreground": "#7f95a1",
    "list.activeSelectionIconForeground": "#7f95a1",
    "list.activeSelectionBackground": "#90c2dc",
    "list.activeSelectionForeground": "#ffffff",
    "list.inactiveSelectionBackground": "#90c2dc55",
    "list.inactiveSelectionForeground": "#7f95a1",
    "list.hoverBackground": "#90c2dc33",
    "editorGroupHeader.tabsBackground": "#f4fbff",
    "titleBar.activeBackground": "#e6f2f9",
    "titleBar.inactiveBackground": "#e6f2f9"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#dc90a5"
      }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": {
        "foreground": "#b090dc"
      }
    },
    {
      "name": "Invalid - Illegal",
      "scope": "invalid.illegal",
      "settings": {
        "foreground": "#FF5733cc"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#7f95a1"
      }
    },
    {
      "name": "Keywords",
      "scope": ["keyword", "storage"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#7f95a1"
      }
    },
    {
      "name": "Types",
      "scope": ["storage.type", "support.type"],
      "settings": {
        "foreground": "#7f95a1"
      }
    },
    {
      "name": "Language Constants",
      "scope": ["constant.language", "support.constant", "variable.language"],
      "settings": {
        "foreground": "#7f95a1"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "support.variable"],
      "settings": {
        "foreground": "#7f95a1"
      }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#7f95a1"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.type",
        "entity.other.inherited-class",
        "support.class"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#7f95a1"
      }
    },
    {
      "name": "Exceptions",
      "scope": "entity.name.exception",
      "settings": {
        "foreground": "#FF5733cc"
      }
    },
    {
      "name": "Sections",
      "scope": "entity.name.section",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#7f95a1"
      }
    },
    {
      "name": "Numbers, Characters",
      "scope": ["constant.numeric", "constant.character", "constant"],
      "settings": {
        "foreground": "#7f95a1"
      }
    },
    {
      "name": "Strings: Escape Sequences",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#7f95a1"
      }
    },
    {
      "name": "Strings: Regular Expressions",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#b090dc"
      }
    },
    {
      "name": "Strings: Symbols",
      "scope": "constant.other.symbol",
      "settings": {
        "foreground": "#b090dc"
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation",
      "settings": {
        "foreground": "#7f95a1"
      }
    },
    {
      "name": "Markup Quote",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#b090dc"
      }
    },
    {
      "name": "Markup Inline",
      "scope": "markup.inline.raw",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#b090dc"
      }
    }
  ]
}
